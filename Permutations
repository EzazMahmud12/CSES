#include<bits/stdc++.h>
using namespace std;
#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);
#define endl '\n'
#define test_case int t;cin>>t;while(t--)
 
// const int mx=2e5+123;
// long long a[mx];
// long long dp[mx];
// const int Inf=2e9;
 
 
 
 
int main(){
		optimize()
 
		int n;
 
		vector<int> v1;
		vector<int> v2;
 
		cin>>n;
		if(n==1){
			cout<<"1";
			return 0;
		}
		if(n==4){
			cout<<"2 4 1 3";
			return 0;
		}
		if(n<4){
			cout<<"NO SOLUTION";
			return 0;
		}
 
		for(int i=n; i>=1; i--){
			if(i%2==0) v1.push_back(i);
			else v2.push_back(i);
		}
 
		for(auto it:v1) cout<<it<<" ";
		for(auto it2:v2) cout<<it2<<" ";
 
 }
